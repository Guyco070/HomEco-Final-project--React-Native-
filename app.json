{
  "expo": {
    "name": "HomEco",
    "slug": "HomEco",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/HomEcoLogo.png",
    "splash": {
      "image": "./assets/HomEcoLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gaico10.HomEco"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/HomEcoLogo.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.gaico10.HomEco"
    },
    "web": {
      "favicon": "./assets/HomEcoLogo.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/HomEcoLogo.png",
          "color": "#ffffff",
          "sounds": [
            "./assets/House-bell-sound-effect.wav",
            "./assets/House-bell-sound-effect.wav"
          ]
        }
      ]
    ]
  }
}
